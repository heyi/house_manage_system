H1,H2,H3,H4,H5,H6,H7{
	PADDING:0;
	MARGIN: 0px;
	border:0;
	text-align: left;
	font-family:宋体,微软雅黑;
}
body , table {
    font-size:12px;font-family:宋体,微软雅黑,Arial, Helvetica, clean,sans-serif; line-height:160%;
}

/* a{ color:#F60;}*/
body {
    height: 100%; 
    overflow: hidden;
}

html {
    height: 100%; 
}

.head{ height:61px; background:url(images/head_back.jpg) repeat-x; padding:0
-webkit-border-radius: 10px 10px 0 0px;
     -moz-border-radius: 10px 10px 0 0px;
          border-radius: 10px 10px 0 0px;
}
.logo{ height:61px; width:300px;background:url(images/logo.jpg) no-repeat; float:left;
-webkit-border-radius: 10px 0px 0 0px;
     -moz-border-radius: 10px 0px 0 0px;
          border-radius: 10px 0px 0 0px;
 }
.head .account{ float:right; margin:20px 20px 0 0; color:#fff;font-size: 16px}
.head .pull-right{ margin:15px 10px 0 0}

#footer {
    position:absolute; width: 100%;  bottom:0px; text-align:center; border-top:3px solid #065f79;
    background-image: url(images/btn.gif);
}
.main_body {
    background: #fff; 
    position: absolute; 
    top: 93px; 
    bottom: 22px; 
    right: 0px; 
    left: 0px; 
    padding: 10px; 
    border-radius: 10px; 
    overflow: auto; 
}

ul.nav li.dropdown:hover ul.dropdown-menu{
    display: block;   
}
.navbar .dropdown-menu {
    margin-top: 0px;
}
.page {
    position: relative; 
    background: #fff; 
    margin: auto; 
    border-radius: 10px; 
    height: 100%;
}

.pagination {
    margin: 0 0 0 0;
}
.btn-file {
  position: relative;
  overflow: hidden;
  vertical-align: middle;
}


.navbar .nav > li > a {
  float: none;
  padding: 0px 10px 0px;
  height:25px; line-height:25px;
  color: #0f5b6b;
  text-decoration: none;
  background:url(images/nav.gif) repeat-x #d1ebf2;
  -webkit-border-radius: 3px 3px 0 0;
     -moz-border-radius: 3px 3px 0 0;
          border-radius: 3px 3px 0 0;
}
.dropdown-menu {border:1px solid #b8dbf1;background-color: #f5fdff;}

textarea,
input[type="text"],
input[type="password"],
input[type="datetime"],
input[type="datetime-local"],
input[type="date"],
input[type="month"],
input[type="time"],
input[type="week"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"],
.uneditable-input{ border:1px solid #b8dbf1; color:#286aa7;}

.breadcrumb{background:url(images/g.gif) repeat-x; border-bottom:2px solid #b8dbf1;
-webkit-border-radius: 4px 4px 0 0;
     -moz-border-radius: 4px 4px 0 0;
          border-radius: 4px 4px 0 0;
          margin-bottom: 10px; 
}

.table-bordered {
  border: 1px solid #b8dbf1;
  border-collapse: separate;
  *border-collapse: collapse;
  border-left: 0;
  -webkit-border-radius: 4px;
     -moz-border-radius: 4px;
          border-radius: 4px;
}

.table th,
.table td {
  padding: 5px;
  line-height: 20px;
  text-align: left;
  vertical-align: top;
  border-top: 1px solid #c5ecf9;
}

.table-bordered th,
.table-bordered td {
  border-left: 1px solid #c5ecf9;
}
.table-bordered th{background:url(images/g.gif) repeat-x; color:#286aa7 }
.accordion-group {
  margin-bottom: 2px;
  border: 1px solid #c5ecf9;
  -webkit-border-radius: 4px;
     -moz-border-radius: 4px;
          border-radius: 4px;
}
.accordion-heading {background:url(images/g.gif) repeat-x; color:#286aa7;}
.accordion-inner {
    border-top: 1px solid #c5ecf9;
    padding: 9px 15px;
}

.nav-tabs {
    border-bottom: 1px solid #c5ecf9;
}
.nav-tabs > .active > a, .nav-tabs > .active > a:hover {

    background-color: #fff;
    border-color: #c5ecf9 #c5ecf9 transparent;
    color: #286aa7;

}

.pagination ul > li > a, .pagination ul > li > span {
    background:url(images/btn.gif) repeat-x; color:#286aa7;
    border-color: #c5ecf9;
}
.pagination ul > li > a:hover{
    color:#F60;
}

.pagination ul > .active > a, .pagination ul > .active > span {
background:url(images/btn_hover.gif) repeat-x;color:#F60;}
.navbar .nav > li {
    float: left;
    margin: 0 2px;
}
.navbar-inner {
    min-height: 25px;
    padding-top: 5px;
    padding-right: 10px;
    padding-left: 10px;
    background: rgb(89, 195, 215);
    border-top : none !important;
    border-left : none !important;
    border-right : none !important;
    border-radius: 0px !important;
    border-bottom: 1px solid rgb(89, 195, 215);
}
select {
    border:1px solid rgb(184, 219, 241);
}
.modal .table td:nth-child(odd) {
    text-align: right;
}
.modal-header {
    background:url(images/btn.gif) repeat-x #e3f5f9; border-bottom:1px solid #b8dbf1; color:#286aa7;
    padding: 5px 10px 10px;
    border-radius: 6px 6px 0px 0px;
}
.datepicker {
    z-index: 2000;
}
caption {
    text-align: left;
    font-weight: bold;
    padding: 5px 0px;
    color: rgb(14, 45, 95);
    border-color: rgb(149, 184, 231);
    background-color:rgb(149, 184, 231);
}

.table-outer {
    position: relative;
    width: 820px;
    margin: auto;
    padding: 32px 2px 0px 2px;
}
.table-outer .table {
    width: 800px;
}

.table-inner {
    overflow: auto;
}

.table-outer thead tr {
    position: absolute;
    top: 1px;
    left: 2px;
}
.table-outer th, .table-outer td {
    width: 200px;
    text-align: left;
}
.table-outer .table {
    border-top:none;
}
