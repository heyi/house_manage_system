.grid{ margin:auto;}
.grid .date_grid{ width:290px; height:120px; padding:180px 0 0 10px; float:left; margin:0 20px 20px 0; background:url(images/c2.png); position:relative }
.date_grid .w_ico{position:absolute; left:20px; top:20px; width:60px; height:60px;}
.date_grid .tp{ text-align:left; font-family:微软雅黑; font-size:18px; color:#fff;text-shadow: 0 0 3px #000; line-height:160%;position:absolute; left:100px; top:30px; }
.date_grid .loc{ text-align:left; font-family:微软雅黑; font-size:18px; color:#fff;text-shadow: 0 0 3px #000; line-height:160%;position:absolute; left:210px; top:28px; }
.date_grid .date{ font-size:100px; line-height:100px; text-align:left; color:#FC0;text-shadow: 0 0 3px #000; position:absolute; left:10px; bottom:10px;}
.date_grid .day{ text-align:left; font-family:微软雅黑; font-size:18px; color:#fff;text-shadow: 0 0 3px #000; line-height:160%;position:absolute; left:130px; bottom:20px; }

.grid a{ display:block; width:140px;height:120px; padding:20px 0px 0 0;  float:left; margin:0 20px 20px 0; background:url(images/c_back.png) no-repeat; font-size:18px; text-align:center; font-family:微软雅黑; color:#fff; text-shadow: 0px 0px 3px #000;}
.grid a:hover{ background-image:url(images/c_back_hover.png); text-decoration:none; color:#FC0; }

.arrow_left{ display:block;width:23px; height:36px; background:url(images/arrow.png); position:absolute; left:85px; top:50%;}
.arrow_right{ display:block;width:23px; height:36px; background:url(images/arrow.png) -23px 0; position:absolute; right:85px; top:50%;}
.arrow_left:hover{ background-position:-46px 0;}
.arrow_right:hover{ background-position:-69px 0;}

.i_big{ display:block; margin:0 auto 10px; width:60px; height:60px; background:url(images/i_big.png);}
.i_big#i2{ background-position:-60px 0}
.i_big#i3{ background-position:-120px 0}
.i_big#i4{ background-position:-180px 0}
.i_big#i5{ background-position:-240px 0}
.i_big#i6{ background-position:-300px 0}
.i_big#i7{ background-position:-360px 0}
.i_big#i8{ background-position:-420px 0}
.i_big#i9{ background-position:-480px 0}
.i_big#i10{ background-position:-540px 0}
.i_big#i11{ background-position:-600px 0}

