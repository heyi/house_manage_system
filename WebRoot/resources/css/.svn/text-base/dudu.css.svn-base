.btn{ background:url(images/btn.gif) repeat-x #e3f5f9; border:1px solid #b8dbf1; color:#286aa7;}
.btn:hover,#port_list.btn:active,
.btn.active{ background:url(images/btn_hover.gif) repeat-x;color:#286aa7; 
    -webkit-box-shadow:none;
    -moz-box-shadow: none;
    box-shadow:none;
}

.dropdown-menu {
    border: 1px solid rgb(184, 219, 241);
    background-color: rgb(245, 253, 255);
}

.btn-group > .btn, .btn-group > .dropdown-menu {
    font-size: 14px;
}

.form_validate {
    position: absolute; 
    top: -50%; 
    left: 50%; 
    width: 400px; 
    z-index: 4999; 
    background-color: rgb(245, 253, 255);
    border-radius: 10px; 
    margin-left :  -200px; 
    color: rgb(185, 74, 72);
    padding: 10px; 
}
.form_validate.active {
    top: 35%; 
}
#ajax_loading {
    background: url(images/loading.gif);
    width: 40px;
    height: 40px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -20px 0 0 -20px;
    display: none;
    z-index: 5999;
}
.c_video, .c_remote, .c_workstatus, .c_historydata {
    display: block; 
    float: left; 
    cursor: pointer; 
    margin-right: 5px; 
    width: 24px; 
    height: 24px; 
}
.c_remote {
    background: url(images/icon_remotectrl.png); 
}
.c_video {
    background: url(images/icon_camera.png); 
}
.c_workstatus {
    background: url(images/i_signal_no.png); 
}

.c_historydata {
    background: url(images/i_historydata.png); 
}

